public class Stack {
    public List<Object> stack = new List<Object>();
    
    public void push(Object obj) {
        stack.add(obj);
    }
    
    public Object pop() {
        if(isEmpty()) {
            return null;
        }        
        return stack.remove(size() - 1);
    }
    
    public Integer size() {
        return stack.size();
    }
    
    public Object peek(){
        return isEmpty() ? null : stack[stack.size() - 1];
    }
    
    public Boolean isEmpty() {
        return stack.isEmpty();
    }
}