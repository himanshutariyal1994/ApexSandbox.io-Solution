public Boolean companionPlants(String plant1, String plant2) {
  //code here
  Map<String, String> companionPlantsMap = new Map<String, String>{
    'cucumbers' => 'lettuce',
    'onions' => 'lettuce',
    'carrots' => 'onions',
    'tomatoes' => 'onions'
  };

  return ((companionPlantsMap.get(plant1) == plant2) ||
  (companionPlantsMap.get(plant2) == plant1));
}
