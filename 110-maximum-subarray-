public static Integer maxSubArraySum(List<Integer> nums, Integer target){
    Integer maxSum = 0;
    Integer currSum = 0;

    for(Integer i = 0;i<target;i++) {
        maxSum += nums[i];
    }
    currSum = maxSum;

    for(Integer i = target;i<nums.size();i++) {
        currSum = currSum + target[i] - target[i - target];
        if(currSum > maxSum) {
            maxSum = currSum;
        }
    }
    return maxSum;
}