public void plusOne(List<Integer> numbers) {
    if(numbers == null || numbers.isEmpty()) {
        return;
    }

    for(Integer i = numbers.size() - 1;i>=0;i--) {
        numbers[i]++;

        if(numbers[i] <= 9) {
            return;
        }
        numbers[i] = 0;
    }
    numbers.add(0,1);
}