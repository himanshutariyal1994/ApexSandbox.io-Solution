public Integer secondLargest(List<Integer> numbers) {
  Integer max = -2147483647 - 1;
  Integer secondMax = -2147483647 - 1;

  for (Integer num : numbers) {
    if (num > max) {
      secondMax = max;
      max = num;
    } else if (max == num) {
      continue;
    } else {
      if (secondMax < num) {
        secondMax = num;
      }
    }
  }
  return secondMax;
}
