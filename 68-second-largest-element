public Integer secondLargest(List<Integer> numbers) {
    Integer max = -Integer.valueOf(Math.pow(2,31)) - 1;
    Integer secondMax = -Integer.valueOf(Math.pow(2,31)) - 1;

    for(Integer num : numbers) {
        if(num > max) {
            secondMax = max;
            max = num;
        } else {
            if(num > secondMax) {
                secondMax = num;
            }
        }
    }
    return secondMax;
}
