public void defragging(Integer[] memory){
    if(memory == null || memory.isEmpty()) {
        return memory;
    }

    Integer left = 0;
    Integer i = 0;
    Integer size = memory.size();

    while(i < size) {
        if(memory[i] != -1) {
            memory[left++] = memory[i];
        }
    }

    while(left < size) {
        memory[left++] = -1;
    }
}